# AWS Deployment environment
AWS_REGION=ap-southeast-1
# Leave empty to use your AWS profile
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# Database Settings
SETTING_TABLE=aibox_setting
SESSION_TABLE=aibox_session
RETENTION_DAYS=30

# Cognito Settings
USER_POOL_ID=ap-southeast-1_ABCDE23F
CLIENT_ID=123sltslcc9dpo0f1234abcd

# Server Settings
SERVER_HOST=localhost
SERVER_PORT=8080
DEBUG=True

# Security Settings
SECRET_KEY=aibox-production-secret-key-2025
TOKEN_EXPIRATION=3600
SSL_ENABLED=False

# CORS Settings
CORS_ORIGINS=*
CORS_METHODS=GET,POST,PUT,DELETE,OPTIONS
CORS_HEADERS=*

# LLM Provider Settings
# Bedrock
BEDROCK_REGION=us-west-2
# Gemini
GEMINI_SECRET_ID=gemini_api_key
# OpenAI
OPENAI_SECRET_ID=openai_api_key

# AgentCore Runtime (optional)
# Set USE_AGENTCORE=true to route Assistant module to remote AgentCore Runtime
# instead of running the agent locally
USE_AGENTCORE=false
AGENTCORE_RUNTIME_ARN=arn:aws:bedrock-agentcore:ap-southeast-1:YOUR_ACCOUNT_ID:runtime/YOUR_RUNTIME_ID
